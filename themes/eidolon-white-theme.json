  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6A737D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords (const, let, if, for, function, class, etc.)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.declaration"
      ],
      "settings": {
        "foreground": "#9486e4"
      }
    },
    {
      "name": "Control Keywords (italic)",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#9486e4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings, Template Strings, Import/Export Sources",
      "scope": [
        "string",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "string.template",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#74d2b7"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#87bff7"
      }
    },
    {
      "name": "Function and Method Names",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#e2b88d"
      }
    },
    {
      "name": "Types, Classes, Interfaces, Enums",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "storage.type.primitive.ts"
      ],
      "settings": {
        "foreground": "#c45a99"
      }
    },
    {
      "name": "Variables, Properties, Object Keys",
      "scope": [
        "variable",
        "support.variable",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#2E3440"
      }
    },
    {
      "name": "Readonly Variables",
      "scope": "variable.other.constant, variable.other.readonly",
      "settings": {
        "foreground": "#4C566A"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#88a4f7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#bd4277"
      }
    },
    {
      "name": "Language Constants (true, false, null, undefined)",
      "scope": [
        "constant.language",
        "constant.language.undefined.js",
        "constant.language.null.js"
      ],
      "settings": {
        "foreground": "#2E3440",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tags (HTML/JSX)",
      "scope": [
        "entity.name.tag.jsx",
        "entity.name.tag.tsx",
        "entity.name.tag.html"
      ],
      "settings": {
        "foreground": "#e5dca4"
      }
    },
    {
      "name": "Attributes/Props (HTML/JSX)",
      "scope": [
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#9be9f8"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter",
        "meta.bracket"
      ],
      "settings": {
        "foreground": "#4C566A"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#7ed7c0"
      }
    },
    {
      "name": "Decorator / Annotation",
      "scope": [
        "meta.decorator punctuation.decorator",
        "meta.decorator entity.name.function",
        "storage.type.annotation"
      ],
      "settings": {
        "foreground": "#e2b88d",
        "fontStyle": "italic"
      }
    }
  ]
}