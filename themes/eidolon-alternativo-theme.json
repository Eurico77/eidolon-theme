  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#63658b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#87bff7"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#8fc8fa"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#e2b88d"
      }
    },
    {
      "name": "Language Constants (true, false, null)",
      "scope": [
        "constant.language",
        "constant.other.enum.php"
      ],
      "settings": {
        "foreground": "#9f95e9"
      }
    },
    {
      "name": "Other Constants (characters, symbols)",
      "scope": [
        "constant.character",
        "constant.other",
        "support.constant"
      ],
      "settings": {
        "foreground": "#aaecf8"
      }
    },
    {
      "name": "Variables, Properties",
      "scope": [
        "variable",
        "support.variable",
        "meta.definition.variable.js",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#9486e4"
      }
    },
    {
      "name": "Readonly Variables/Properties",
      "scope": "variable.other.constant, variable.other.readonly",
      "settings": {
        "foreground": "#9f95e9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "'this', 'self', 'super'",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#d864aa"
      }
    },
    {
      "name": "Keywords (general, new, expression, logical, word)",
      "scope": [
        "keyword",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.word"
      ],
      "settings": {
        "foreground": "#74d2b7"
      }
    },
    {
      "name": "Control Keywords (if, else, for, while)",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#7ed7c0"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#94aef9"
      }
    },
    {
      "name": "Storage Types/Modifiers (let, const, var, static, public, fn, function)",
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.declaration"
      ],
      "settings": {
        "foreground": "#74d2b7"
      }
    },
    {
      "name": "Types, Classes, Structs, Enums, Traits",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type.primitive",
        "entity.name.struct",
        "entity.name.enum",
        "entity.name.trait",
        "meta.primitive.rust"
      ],
      "settings": {
        "foreground": "#9be9f8"
      }
    },
    {
      "name": "Function and Method Names",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#e5dca4"
      }
    },
    {
      "name": "Function/Method Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c45a99"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#bd4277"
      }
    },
    {
      "name": "HTML/XML Attribute Names",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#d864aa"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name.css"
      ],
      "settings": {
        "foreground": "#9be9f8"
      }
    },
    {
      "name": "CSS Values (strings, numbers, keywords)",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css",
        "constant.other.color.css"
      ],
      "settings": {
        "foreground": "#87bff7"
      }
    },
    {
      "name": "CSS Units",
      "scope": "keyword.other.unit.css",
      "settings": {
        "foreground": "#d864aa"
      }
    },
    {
      "name": "JSON Keys",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#9be9f8"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter",
        "meta.bracket"
      ],
      "settings": {
        "foreground": "#a0a3cc"
      }
    },
    {
      "name": "Object Literal Keys / Property Names (JS/TS)",
      "scope": "meta.object-literal.key, meta.property.object",
      "settings": {
        "foreground": "#9be9f8"
      }
    },
    {
      "name": "Decorator / Annotation",
      "scope": [
        "meta.decorator punctuation.decorator",
        "meta.decorator entity.name.function",
        "storage.type.annotation"
      ],
      "settings": {
        "foreground": "#ecdfac",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": "markup.heading, entity.name.section.markdown",
      "settings": {
        "foreground": "#74d2b7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link.markdown",
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#87bff7"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "foreground": "#e5dca4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": "markup.italic.markdown",
      "settings": {
        "foreground": "#e5dca4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#94aef9"
      }
    },
    {
      "name": "Rust: Macro Invocation",
      "scope": "entity.name.function.macro.rust",
      "settings": {
        "foreground": "#ecdfac"
      }
    },
    {
      "name": "Rust: Module Path",
      "scope": "entity.name.module.rust",
      "settings": {
        "foreground": "#9f95e9"
      }
    },
    {
      "name": "Rust: Trait",
      "scope": "entity.name.type.trait.rust",
      "settings": {
        "foreground": "#7ed7c0"
      }
    },
    {
      "name": "Rust: Lifetime",
      "scope": "entity.name.lifetime.rust",
      "settings": {
        "foreground": "#c45a99",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Rust: Keyword 'crate', 'super', 'self type'",
      "scope": [
        "keyword.other.crate.rust",
        "entity.name.super.rust",
        "entity.name.type.self.rust"
      ],
      "settings": {
        "foreground": "#74d2b7"
      }
    },
    {
      "name": "Rust: Attributes",
      "scope": [
        "meta.attribute.rust",
        "meta.attribute.rust punctuation.definition.attribute.rust"
      ],
      "settings": {
        "foreground": "#aaecf8",
        "fontStyle": "italic"
      }
    }
  ]
}