  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#63658b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings, Template Strings, Import/Export Sources",
      "scope": [
        "string",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "string.template",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "string.quoted.double.json",
        "string.quoted.single.js",
        "string.quoted.double.js",
        "string.quoted.single.ts",
        "string.quoted.double.ts",
        "meta.import.js string.quoted",
        "meta.import.ts string.quoted",
        "meta.export.js string.quoted",
        "meta.export.ts string.quoted"
      ],
      "settings": {
        "foreground": "#87bff7"
      },
      {
        "name": "Regular Expressions",
        "scope": "string.regexp",
        "settings": {
          "foreground": "#8fc8fa"
        },
        {
          "name": "Numbers",
          "scope": "constant.numeric",
          "settings": {
            "foreground": "#e2b88d"
          },
          {
            "name": "Language Constants (true, false, null, undefined), Other Constants",
            "scope": [
              "constant.language",
              "constant.other.enum.php",
              "constant.language.undefined.js",
              "constant.language.null.js"
            ],
            "settings": {
              "foreground": "#9f95e9"
            },
            {
              "name": "Character Constants, Symbols, Support Constants",
              "scope": [
                "constant.character",
                "constant.other",
                "support.constant"
              ],
              "settings": {
                "foreground": "#aaecf8"
              },
              {
                "name": "Variables, Properties, Object Keys",
                "scope": [
                  "variable",
                  "support.variable",
                  "meta.definition.variable.js",
                  "meta.definition.property.js",
                  "support.type.property-name",
                  "meta.object-literal.key",
                  "support.variable.property.js",
                  "support.variable.property.ts"
                ],
                "settings": {
                  "foreground": "#9486e4"
                },
                {
                  "name": "Readonly Variables/Properties",
                  "scope": "variable.other.constant, variable.other.readonly",
                  "settings": {
                    "foreground": "#9f95e9",
                    "fontStyle": "italic"
                  },
                  {
                    "name": "'this', 'self', 'super', 'module', 'console'",
                    "scope": [
                      "variable.language.this",
                      "variable.language.self",
                      "variable.language.super",
                      "support.variable.object.module.js",
                      "support.variable.object.process.js",
                      "support.variable.object.console.js",
                      "support.variable.object.module.ts",
                      "support.variable.object.process.ts",
                      "support.variable.object.console.ts"
                    ],
                    "settings": {
                      "foreground": "#d864aa",
                      "fontStyle": "italic"
                    },
                    {
                      "name": "Keywords (general, new, expression, logical, word, as, from, get, set, async, await, of)",
                      "scope": [
                        "keyword",
                        "keyword.operator.new",
                        "keyword.operator.expression",
                        "keyword.operator.logical",
                        "keyword.operator.word",
                        "keyword.control.as.js",
                        "keyword.control.as.ts",
                        "keyword.control.from.js",
                        "keyword.control.from.ts",
                        "keyword.control.async.js",
                        "keyword.control.async.ts",
                        "keyword.control.await.js",
                        "keyword.control.await.ts",
                        "keyword.operator.accessor.js",
                        "keyword.operator.of.js",
                        "keyword.operator.of.ts"
                      ],
                      "settings": {
                        "foreground": "#74d2b7"
                      },
                      {
                        "name": "Control Keywords (if, else, for, while, import, export, return, try, catch, switch, case, default, class, extends, function, const, let, var)",
                        "scope": [
                          "keyword.control",
                          "storage.type.function.js",
                          "storage.type.function.ts",
                          "storage.type.class.js",
                          "storage.type.class.ts",
                          "storage.modifier.js",
                          "keyword.control.flow",
                          "keyword.control.conditional",
                          "keyword.control.loop",
                          "keyword.control.module.js",
                          "keyword.control.module.ts",
                          "keyword.control.trycatch.js",
                          "keyword.control.trycatch.ts",
                          "keyword.control.switch.js",
                          "keyword.control.switch.ts"
                        ],
                        "settings": {
                          "foreground": "#7ed7c0",
                          "fontStyle": "italic"
                        },
                        {
                          "name": "Operators",
                          "scope": "keyword.operator",
                          "settings": {
                            "foreground": "#94aef9"
                          },
                          {
                            "name": "Types, Classes, Interfaces, Enums, Type Aliases, Primitives",
                            "scope": [
                              "entity.name.type",
                              "entity.other.inherited-class",
                              "support.class",
                              "support.type.primitive",
                              "entity.name.type.class.js",
                              "entity.name.type.class.ts",
                              "entity.name.type.interface.ts",
                              "entity.name.type.enum.ts",
                              "entity.name.type.alias.ts",
                              "storage.type.primitive.ts",
                              "storage.type.built-in.ts",
                            ],
                            "settings": {
                              "foreground": "#9be9f8"
                            },
                            {
                              "name": "Function and Method Names, Arrow Functions, Support Functions",
                              "scope": [
                                "entity.name.function",
                                "support.function",
                                "meta.function-call entity.name.function",
                                "meta.function.arrow.js",
                                "meta.function.arrow.ts",
                                "support.function.console.js",
                                "support.function.module.js",
                                "support.function.process.js",
                                "support.function.console.ts",
                                "support.function.module.ts",
                                "support.function.process.ts"
                              ],
                              "settings": {
                                "foreground": "#e5dca4"
                              },
                              {
                                "name": "Function/Method Parameters",
                                "scope": "variable.parameter",
                                "settings": {
                                  "foreground": "#c45a99",
                                  "fontStyle": "italic"
                                },
                                {
                                  "name": "JSX/HTML Tags",
                                  "scope": [
                                    "entity.name.tag.jsx",
                                    "entity.name.tag.tsx",
                                    "entity.name.tag.html",
                                    "entity.name.tag.xml"
                                  ],
                                  "settings": {
                                    "foreground": "#bd4277"
                                  },
                                  {
                                    "name": "JSX/HTML Attribute Names / Props",
                                    "scope": [
                                      "entity.other.attribute-name.jsx",
                                      "entity.other.attribute-name.tsx",
                                      "entity.other.attribute-name.html",
                                      "entity.other.attribute-name.xml",
                                      "entity.other.attribute-name.id.css"
                                    ],
                                    "settings": {
                                      "foreground": "#d864aa"
                                    },
                                    {
                                      "name": "CSS Property Names",
                                      "scope": [
                                        "support.type.property-name.css",
                                        "meta.property-name.css"
                                      ],
                                      "settings": {
                                        "foreground": "#9be9f8"
                                      },
                                      {
                                        "name": "CSS Values",
                                        "scope": [
                                          "support.constant.property-value.css",
                                          "meta.property-value.css",
                                          "constant.other.color.css"
                                        ],
                                        "settings": {
                                          "foreground": "#87bff7"
                                        },
                                        {
                                          "name": "CSS Units",
                                          "scope": "keyword.other.unit.css",
                                          "settings": {
                                            "foreground": "#e2b88d"
                                          },
                                          {
                                            "name": "JSON Keys",
                                            "scope": "support.type.property-name.json",
                                            "settings": {
                                              "foreground": "#9be9f8"
                                            },
                                            {
                                              "name": "Punctuation (commas, colons, semicolons, brackets, braces, parentheses)",
                                              "scope": [
                                                "punctuation",
                                                "meta.brace",
                                                "meta.delimiter",
                                                "meta.bracket",
                                                "punctuation.separator",
                                                "punctuation.terminator",
                                                "punctuation.accessor"
                                              ],
                                              "settings": {
                                                "foreground": "#a0a3cc"
                                              },
                                              {
                                                "name": "Decorator / Annotation",
                                                "scope": [
                                                  "meta.decorator punctuation.decorator",
                                                  "meta.decorator entity.name.function",
                                                  "storage.type.annotation"
                                                ],
                                                "settings": {
                                                  "foreground": "#ecdfac",
                                                  "fontStyle": "italic"
                                                },
                                                {
                                                  "name": "Markdown Headings",
                                                  "scope": "markup.heading, entity.name.section.markdown",
                                                  "settings": {
                                                    "foreground": "#74d2b7",
                                                    "fontStyle": "bold"
                                                  },
                                                  {
                                                    "name": "Markdown Links",
                                                    "scope": [
                                                      "markup.underline.link.markdown",
                                                      "string.other.link.title.markdown",
                                                      "string.other.link.description.markdown"
                                                    ],
                                                    "settings": {
                                                      "foreground": "#87bff7"
                                                    },
                                                    {
                                                      "name": "Markdown Bold",
                                                      "scope": "markup.bold.markdown",
                                                      "settings": {
                                                        "foreground": "#e5dca4",
                                                        "fontStyle": "bold"
                                                      },
                                                      {
                                                        "name": "Markdown Italic",
                                                        "scope": "markup.italic.markdown",
                                                        "settings": {
                                                          "foreground": "#e5dca4",
                                                          "fontStyle": "italic"
                                                        },
                                                        {
                                                          "name": "Markdown Code",
                                                          "scope": [
                                                            "markup.inline.raw.string.markdown",
                                                            "markup.fenced_code.block.markdown"
                                                          ],
                                                          "settings": {
                                                            "foreground": "#94aef9"
                                                          }
                                                        ]
                                                      }