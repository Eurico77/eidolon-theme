  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#63658b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords (Control Flow, etc.)",
      "scope": [
        "keyword.control",
        "keyword.operator.logical",
        "keyword.operator.word",
        "keyword.operator.new",
        "keyword.operator.expression"
      ],
      "settings": {
        "foreground": "#9486e4"
      }
    },
    {
      "name": "Storage Modifiers, Declarations (const, let, var, function, class, fn, type, struct, enum, trait, impl, use, mod)",
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.declaration",
        "keyword.other.rust",
        "entity.name.function.declaration",
        "entity.name.type.class",
        "entity.name.type.struct",
        "entity.name.type.enum",
        "entity.name.type.trait"
      ],
      "settings": {
        "foreground": "#9f95e9"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#d0d3ff"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#d4d7ff"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#d7daff"
      }
    },
    {
      "name": "Language Constants (true, false, null), Other Constants",
      "scope": [
        "constant.language",
        "constant.other.enum.php",
        "constant.other",
        "support.constant"
      ],
      "settings": {
        "foreground": "#daddff"
      }
    },
    {
      "name": "Variables, Properties",
      "scope": [
        "variable",
        "support.variable",
        "meta.definition.variable.js",
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#cccfff"
      }
    },
    {
      "name": "'this', 'self', 'super'",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#cccfff"
      }
    },
    {
      "name": "Operators (non-keyword)",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#a0a3cc"
      }
    },
    {
      "name": "Types (non-declaration), Support Classes",
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type.primitive",
        "meta.primitive.rust"
      ],
      "settings": {
        "foreground": "#d7daff"
      }
    },
    {
      "name": "Function and Method Names (invocations), Support Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#daddff"
      }
    },
    {
      "name": "Function/Method Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#cccfff"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#d0d3ff"
      }
    },
    {
      "name": "HTML/XML Attribute Names",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#d4d7ff"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name.css"
      ],
      "settings": {
        "foreground": "#d7daff"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css",
        "constant.other.color.css"
      ],
      "settings": {
        "foreground": "#d0d3ff"
      }
    },
    {
      "name": "CSS Units",
      "scope": "keyword.other.unit.css",
      "settings": {
        "foreground": "#d4d7ff"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter",
        "meta.bracket"
      ],
      "settings": {
        "foreground": "#595b7e"
      }
    },
    {
      "name": "Decorator / Annotation",
      "scope": [
        "meta.decorator punctuation.decorator",
        "meta.decorator entity.name.function",
        "storage.type.annotation",
        "meta.attribute.rust"
      ],
      "settings": {
        "foreground": "#daddff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Rust: Module Path",
      "scope": "entity.name.module.rust",
      "settings": {
        "foreground": "#d0d3ff"
      }
    },
    {
      "name": "Rust: Lifetime",
      "scope": "entity.name.lifetime.rust",
      "settings": {
        "foreground": "#cccfff",
        "fontStyle": "italic"
      }
    }
  ]
}